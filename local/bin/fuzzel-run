#!/usr/bin/env bash
#
# Launch fuzzel
# USAGE: fuzzel-run


fuzzel-run() {
  local -a fuzzel_opts

  fuzzel_opts=(
    -I
    -f 'Overpass'
    -T 'footclient'
    -b '23222bff'
    -s '474557ff'
    -t 'eae9eeff'
    -m 'e8ec77ff'
    -C '57bbf4ff'
    -x 10 -y 5 -r 4 -l 5
  )

  fuzzel "${fuzzel_opts[@]}" <&0
}

fuzzel-run "$@"

